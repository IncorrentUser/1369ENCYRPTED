--[[
	WARNING: Risk kullanan kişiye aittir.
]]
local delay = 2 
function send(text)
   local A_delay = text
   local A_2 = "All"
   local event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
   event:FireServer(A_delay, A_2)
end

local player = game.Players.LocalPlayer
local humanoid = player.Character:FindFirstChildOfClass("Humanoid")

while wait(delay) do
   send("BİR")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("İKİ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("ÜÇ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("DÖRT")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("BEŞ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("ALTI")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("YEDİ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("SEKİZ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("DOKUZ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("ON")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   -- Aynı yapı 50'ye kadar devam eder

   send("KIRK DOKUZ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
   
   send("ELLİ")
   humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
   wait(delay)
end

inform("Oyun algılandı... Tür: JJ", 3)
wait(3.2)
inform("Members of the Special Execution Team Organization | 1 3 6 9 |", 2)
